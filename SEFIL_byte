e.RenderStepped:Connect(function()local ar=a.Character or a.CharacterAdded:Wait()local as=ar:WaitForChild("Humanoid")local at=h and i or j
as.WalkSpeed=at
p.Position=UDim2.new(0,b.X,0,b.Y)
if p and p.Character then local au=p.Character.HumanoidRootPart.Position
c.CFrame=CFrame.new(c.CFrame.Position,au)end end)end
local function av(aw)if not aw or not aw:FindFirstChild("HumanoidRootPart") then return nil end
local ax=aw.HumanoidRootPart.Position
local ay=f.LocalPlayer:GetNetworkPing()local az=(ax-c.CFrame.Position).Magnitude/100
local ba=ax+(aw.HumanoidRootPart.Velocity*(ay/1000+az)) return ba end
local function bb(bc)local bd=50 local be=Vector3.new(b.X,b.Y,b.Z)local bf=(be-bc).Magnitude return bf<=bd end
local function bg()local bh=nil
local bi=math.huge
for _,bj in ipairs(f:GetPlayers())do if bj~=a and bj.Character and bj.Character:FindFirstChild("HumanoidRootPart")then local bk=bj.Character.HumanoidRootPart.Position
if bb(bk)then local bl=(c.CFrame.Position-bk).Magnitude if bl<bi then bi=bl bh=bj end end end end
if bh then p=bh end end
local function bm()if d:IsKeyDown(k)then h=true else h=false end
bg()end
t()e.RenderStepped:Connect(bm)
