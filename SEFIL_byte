local g=a:WaitForChild("PlayerGui")
local h=false
local i=50
local j=16
local k=Enum.KeyCode.LeftShift
local l=Enum.KeyCode.RightControl
local m={n=Enum.KeyCode.E,o=Enum.KeyCode.Q}
local p=nil
local q
local r
local s
local t=function()
if q then q:Destroy()end
q=Instance.new("ScreenGui",g)q.Name="RandomGui"q.ResetOnSpawn=false
