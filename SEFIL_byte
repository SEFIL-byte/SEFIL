local function ab()local ac=w.Position.X.Scale
i=math.floor(ac*100)u.Text="Speed: "..i end
w.MouseButton1Down:Connect(function()local ad
ad=e.RenderStepped:Connect(function()local ae=d:GetMouseLocation().X-v.AbsolutePosition.X
local af=math.clamp(ae,0,v.AbsoluteSize.X)local ag=af/v.AbsoluteSize.X
w.Position=UDim2.new(ag,0,0,0)ab()end)
d.InputEnded:Connect(function(ah)if ah.UserInputType==Enum.UserInputType.MouseButton1 then ad:Disconnect()end end)end)
x.MouseButton1Click:Connect(function()x.Text="Press a key..."local ai
ai=d.InputBegan:Connect(function(aj,ak)if ak then return end
m.n=aj.KeyCode
x.Text="Lock Key: "..m.n.Name
ai:Disconnect()end)end)
y.MouseButton1Click:Connect(function()y.Text="Press a key..."local al
